<launch>
  <!-- Alte noduri și parametri -->

  <node name="pollination_planner" pkg="pollibee_drone_simulation" type="pollination_planner.py" output="screen">
    <param name="field_size" value="[10, 10, 2]"/>
    <param name="grid_size" value="1.0"/>
    <param name="hover_height" value="1.5"/>
  </node>

  <node name="drone_controller" pkg="pollibee_drone_simulation" type="drone_controller.py" output="screen">
    <param name="max_velocity" value="1.0"/>
    <param name="position_tolerance" value="0.1"/>
  </node>
<node pkg="pollibee_drone_simulation" type="flower_detection.py" name="flower_detection" output="screen">
</node>

<node name="yolo_detector" pkg="pollibee_drone_simulation" type="yolo_detector.py" output="screen">
    <param name="yolo_config" value="$(find pollibee_drone_simulation)/config/yolov3.cfg"/>
    <param name="yolo_weights" value="$(find pollibee_drone_simulation)/config/yolov3.weights"/>
    <param name="yolo_classes" value="$(find pollibee_drone_simulation)/config/coco.names"/>
    <param name="confidence_threshold" value="0.5"/>
    <param name="nms_threshold" value="0.4"/>
  </node>

<node pkg="pollibee_drone_simulation" type="yolo_detection.py" name="yolo_detection" output="screen">
    <param name="confidence_threshold" value="0.5"/>
    <param name="nms_threshold" value="0.4"/>
    <param name="yolo_model" value="$(find pollibee_drone_simulation)/models/yolov3.weights"/>
    <param name="yolo_config" value="$(find pollibee_drone_simulation)/models/yolov3.cfg"/>
    <param name="classes_file" value="$(find pollibee_drone_simulation)/models/coco.names"/>
</node>

<node pkg="pollibee_drone_simulation" type="control_drone.py" name="drone_controller" output="screen">
    <param name="max_speed" value="0.5"/>
    <param name="min_distance" value="1.0"/>
</node>
<node pkg="pollibee_drone_simulation" type="path_planning.py" name="path_planning" output="screen">
  <param name="update_rate" value="1"/>
</node>
<node pkg="pollibee_drone_simulation" type="pollination_motor.py" name="pollination_motor" output="screen">
    <param name="pollination_duration" value="2.0"/>
</node>
<node pkg="pollibee_drone_simulation" type="data_logging.py" name="data_logger" output="screen">
    <param name="log_file" value="$(find pollibee_drone_simulation)/data/simulation_log.csv"/>
</node>
<node pkg="pollibee_drone_simulation" type="visualize_results.py" name="visualizer" output="screen"/>
<node pkg="pollibee_drone_simulation" type="wind_simulation.py" name="wind_simulation" output="screen">
  <param name="wind_mean" value="[1.0, 0.0, 0.0]"/>
  <param name="wind_variance" value="0.5"/>
  <param name="update_rate" value="10"/>
</node>
<node pkg="pollibee_drone_simulation" type="trajectory_follower.py" name="trajectory_follower" output="screen">
  <param name="update_rate" value="10"/>
  <param name="max_speed" value="1.0"/>
  <param name="goal_tolerance" value="0.1"/>
</node>
  <!-- Alte noduri și parametri -->
</launch>
